{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./",
    "paths": {
      "@layouts/*": ["src/layouts/*"],
      "@modules/*": ["src/modules/*"],
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@interfaces/*": ["src/interfaces/*"],
    }
  }
}